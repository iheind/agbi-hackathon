{
  "expo": {
    "name": "Mehta OCR",
    "slug": "mehta-ocr",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "package": "com.mehtaocr.mehtaocr",
      "versionCode": 2
    },
    "description": "An application that enables the user to convert any form of handwritten or printed text into editable digital text by either directly taking photo of it or by using an image from the gallery."
  }
}
